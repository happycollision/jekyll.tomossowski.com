{
  "name": "tomossowski.com",
  "version": "0.0.1",
  "scripts": {
    "copyFolder": "./scripts/copy_folder.cjs",
    "useCache": "npm run copyFolder -- --merge /opt/build/cache/hc_my_cache/static/g --to static/g",
    "fillCache": "npm run copyFolder -- --save static/g --to /opt/build/cache/hc_my_cache/static/g",
    "clean": "rm -rf ./static/g && rm -rf ./svelte-kit && rm -rf ./build",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "netlify": "npm run useCache && npm run build && npm run fillCache"
  },
  "devDependencies": {
    "@beyonk/svelte-carousel": "^2.8.0",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-image": "matyunya/svelte-image#e41e7b5960f4f108094c3877859b6257b97c2e7a",
    "svelte-preprocess": "^4.7.3",
    "svelte2tsx": "^0.3.0",
    "tailwindcss": "^2.2.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "volta": {
    "node": "16.4.0",
    "npm": "7.18.1"
  }
}
